#define PERK_EDITOR_BUTTON_SINGLE( slotNumber, navArg, headerArg, textArg, visArg ) \
		PREPROC_TEXT_DRAW_VIS_EX( \
			BUTTONS_X (PERK_SLOT1_BUTTON_Y+PERK_EDITOR_BUTTON_SPACING*(slotNumber-1)) PERK_EDITOR_BUTTONS_WIDTH PERK_EDITOR_BUTTON_HEADER_HEIGHT HORIZONTAL_ALIGN_CENTER VERTICAL_ALIGN_CENTER, 0 0, \
			locString( headerArg, slotNumber ), \
			TEXTSIZE_DEFAULT, 0, 0, ITEM_ALIGN_TOP_LEFT, PERK_EDITOR_BUTTON_HEADER_COLOR, \
			when( visArg ), \
			name NEW_FRAME_BUTTON_NAME#slotNumber; NEW_FRAME_TRANSPARENT_STATE( 1 1 1 ) ) \
		PERK_EDITOR_PLATFORM_BUTTON( slotNumber, navArg, textArg, visArg )
			
#define PERK_EDITOR_BUTTON( slotNumber, navArg )																																						\
		PERK_EDITOR_BUTTON_SINGLE( slotNumber, navArg, "@CUSTOM_PERK_SLOT", "@CUSTOM_PERK_EDIT", !dvarBool( custom_perks_allow_pro ) ) \
		PERK_EDITOR_BUTTON_SINGLE( slotNumber, navArg, "@CUSTOM_PERK_PRO_SLOT", "@CUSTOM_PERK_PRO_EDIT", dvarBool( custom_perks_allow_pro ) )

