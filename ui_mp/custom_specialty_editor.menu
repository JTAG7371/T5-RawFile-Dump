#define PERK_EDITOR_SLOT_MOVE( numberArg, prevNumber, nextNumber )	\
		execKeyInt APAD_UP										\
		{														\
			setfocus NEW_FRAME_BUTTON_NAME#prevNumber			\
		}														\
		execKeyInt DPAD_UP										\
		{														\
			setfocus NEW_FRAME_BUTTON_NAME#prevNumber			\
		}														\
		execKeyInt APAD_DOWN									\
		{														\
			setfocus NEW_FRAME_BUTTON_NAME#nextNumber			\
		}														\
		execKeyInt DPAD_DOWN									\
		{														\
			setfocus NEW_FRAME_BUTTON_NAME#nextNumber			\
		}
		
#define PERK_ACTION( index ) \
		execnow "set ui_inside_popup specialties_"#index; \
		execnow "set selected_loadout_slot specialty"#index; \
		execnow buildItemListForSlotName (dvarString( selected_loadout_slot ));	\
		execnow "set list_perks_in_slot "#index; \
		execNow "movefeeder top perk_list_"#index; \
		setfocus "perk_list_"#index;
			
#define PERK_EDITOR_BUTTON_ACTION( slotNumber ) \
		PERK_ACTION( slotNumber ) \
		setdvar list_perks_in_slot slotNumber; \
		execnow ui_animate custom_specialty_editor anim_controller OpenDropDownList 0;	

