menuDef
{
	name					custom_specialty_editor
	rect					0 0 640 480 HORIZONTAL_ALIGN_FULLSCREEN VERTICAL_ALIGN_FULLSCREEN
	focuscolor				COLOR_FOCUSED
	style					WINDOW_STYLE_FILLED
	soundloop 				MENU_MUSIC
	onOpen	
	{
		setdvar ui_items_no_cost 1;
		execnow set list_perks_in_slot 0;
		setdvar ui_custom_perk_has_changed 0;
#ifdef PC
		setlocalVarInt ui_perk_slot_hl 1;
		setdvar ui_choice_noaction 0;
#endif
		activateBlur;
		focusFirst;
	}
	onClose	
	{
		setdvar ui_items_no_cost 0;
		deactivateBlur;
#ifdef PC
		setdvar ui_choice_noaction 1;
#endif
	}
	onESC	
	{
		ON_ESC
	}
	onFocus
	{
	}
	PC_TAB_SWITCH_SAME_MENU_ACTIONS
	
