	PERK_EDITOR_BUTTON( 2, PERK_EDITOR_SLOT_MOVE( 2, 1, 3 ) )
	PERK_EDITOR_LISTBOX( 2, PERK_EDITOR_LIST_HEIGHT_2 )
	PERK_EDITOR_BUTTON( 3, PERK_EDITOR_SLOT_MOVE( 3, 2, 1 ) )
	PERK_EDITOR_LISTBOX( 3, PERK_EDITOR_LIST_HEIGHT_3 )

	itemDef
	{
		name	anim_controller
		style	WINDOW_STYLE_SHADER
		rect	0 0 0 0
		visible 1
		decoration
		
		state
		{
			name	OpenDropDownList
			onEnter
			{
				setFocus dummyButton;
				if ( dvarInt( list_perks_in_slot ) == 1 )
				{
					execnow ui_animate custom_specialty_editor NEW_FRAME_BUTTON_NAME"2" Transparent PERK_EDITOR_BUTTONS_FADE_TIME;
					execnow ui_animate custom_specialty_editor NEW_FRAME_BUTTON_NAME"3" Transparent PERK_EDITOR_BUTTONS_FADE_TIME;
				}
				elseif ( dvarInt( list_perks_in_slot ) == 2 )
				{
					execnow ui_animate custom_specialty_editor NEW_FRAME_BUTTON_NAME"3" Transparent PERK_EDITOR_BUTTONS_FADE_TIME;
				}
				execnow ui_animate custom_specialty_editor anim_controller OpenDropDownList2 (PERK_EDITOR_BUTTONS_FADE_TIME+INTERIM_ANIM_TIME);
			}
		}
		state
		{
			name	OpenDropDownList2
			onEnter
			{
				execnow ui_animate custom_specialty_editor ("perk_list_"+int(dvarInt( list_perks_in_slot ))) Open PERK_EDITOR_LIST_ANIM_TIME;
				execnow ui_animate custom_specialty_editor anim_controller OpenDropDownList3 (PERK_EDITOR_LIST_ANIM_TIME+INTERIM_ANIM_TIME);
			}
		}
		state
		{
			name	OpenDropDownList3
			onEnter
			{
				execnow focusitem ("perk_list_"+int(dvarInt( list_perks_in_slot )));
			}
		}
		
		state
		{
			name	CloseDropDownList
			onEnter
			{
				setFocus dummyButton;
				execnow ui_animate custom_specialty_editor ("perk_list_"+int(dvarInt( list_perks_in_slot ))) Default PERK_EDITOR_LIST_ANIM_TIME;
				execnow ui_animate custom_specialty_editor anim_controller CloseDropDownList2 (PERK_EDITOR_LIST_ANIM_TIME+INTERIM_ANIM_TIME);
			}
		}
		state
		{
			name	CloseDropDownList2
			onEnter
			{
				execnow ui_animate custom_specialty_editor NEW_FRAME_BUTTON_NAME"2" Default PERK_EDITOR_BUTTONS_FADE_TIME;
				execnow ui_animate custom_specialty_editor NEW_FRAME_BUTTON_NAME"3" Default PERK_EDITOR_BUTTONS_FADE_TIME;
				execnow ui_animate custom_specialty_editor anim_controller CloseDropDownList3 (PERK_EDITOR_BUTTONS_FADE_TIME+INTERIM_ANIM_TIME);
			}
		}
		state
		{
			name	CloseDropDownList3
			onEnter
			{
				execnow focusitem (NEW_FRAME_BUTTON_NAME+int(dvarInt( list_perks_in_slot )));
				setdvar	list_perks_in_slot 0;
