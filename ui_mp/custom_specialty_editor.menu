#define PERK_EDITOR_LISTBOX( numberArg, heightArg ) \
		itemDef\
		{ \
			name						"perk_list_"#numberArg \
			type						ITEM_TYPE_LISTBOX \
			feeder						FEEDER_CUSTOM_PERKSINSLOT \
			rect						BUTTONS_X (PERK_SLOT1_BUTTON_Y+PERK_EDITOR_BUTTON_SPACING*(numberArg-1)+PERK_EDITOR_BUTTON_HEADER_HEIGHT+NEW_FRAME_BUTTON_HEIGHT) PERK_EDITOR_BUTTONS_WIDTH 0 HORIZONTAL_ALIGN_CENTER VERTICAL_ALIGN_CENTER \
			origin						0 0 \
			elementwidth				30 \
			elementheight				17 \
			elementtype					LISTBOX_TEXT \
			textstyle					ITEM_TEXTSTYLE_NORMAL \
			textfont					UI_FONT_NORMAL \
			textscale					TEXTSIZE_DEFAULT \
			textalignx					12 \
			forecolor					1 1 1 1 \
			focusColor					1 1 1 1 \
			disableColor				1 1 1 1 \
			PERK_EDITOR_LIST_MODAL \
			noBlinkingHighlight \
			noscrollbars \
			\/*				x					y					w				h					len	horzAlign		vertAlign*/
			userarea	7	-1					0					(LIST_WIDTH+1)	LIST_ELEM_HGT		32	ITEM_ALIGN_LEFT	ITEM_ALIGN_MIDDLE \	/* Background */
							10					0					LIST_WIDTH		LIST_ELEM_HGT		32	ITEM_ALIGN_LEFT	ITEM_ALIGN_MIDDLE \	/* Name */
							(LIST_WIDTH-18)		2					14				14					0	ITEM_ALIGN_LEFT	ITEM_ALIGN_MIDDLE \	/* Custom Icon */
							(LIST_WIDTH-60)		0					50				LIST_ELEM_HGT		24	ITEM_ALIGN_LEFT	ITEM_ALIGN_MIDDLE \	/* Cost */
							(LIST_WIDTH)		(17/2-LOCK_SIZE/2)	LOCK_SIZE		LOCK_SIZE			24	ITEM_ALIGN_LEFT	ITEM_ALIGN_MIDDLE \	/* Lock */
							(LIST_WIDTH-100)	0					50				LIST_ELEM_HGT		24	ITEM_ALIGN_LEFT	ITEM_ALIGN_MIDDLE \	/* Count */
							1					0					16 				LIST_ELEM_HGT		16	ITEM_ALIGN_LEFT	ITEM_ALIGN_MIDDLE \	/* Extra Image */
			visible		when( dvarInt( list_perks_in_slot ) == numberArg ); \
			onFocus \
			{ \
				setLocalVarInt UI_HL_TYPE -1; \
				setLocalVarString ui_choicegroup CHOICE_GROUP; \
			} \
			leaveFocus \
			{ \
			} \
			onListboxSelectionChange \
			{ \
				play CHOICE_FOCUS_SOUND; \
			} \
			doubleclick \
			{ \
				execNow set ui_choice_desc ""; \
				execNow openMenu ( "popup_" + GetCurrentItemName() ); \
			} \
			execKeyInt BUTTON_B \
			{ \
				execnow ui_animate custom_specialty_editor anim_controller CloseDropDownList 0; \
			} \
			state \
			{ \
				name	Open \
				rect	BUTTONS_X (PERK_SLOT1_BUTTON_Y+PERK_EDITOR_BUTTON_SPACING*(numberArg-1)+PERK_EDITOR_BUTTON_HEADER_HEIGHT+NEW_FRAME_BUTTON_HEIGHT) PERK_EDITOR_BUTTONS_WIDTH heightArg HORIZONTAL_ALIGN_CENTER VERTICAL_ALIGN_CENTER \
			} \
		}

